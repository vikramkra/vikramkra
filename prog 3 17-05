	// Initial Template for C++
	#include <bits/stdc++.h>
	using namespace std;
	
	 // } Driver Code Ends
	// User function Template for C++
	class Solution {
	  public:
	    long longintconvertEvenBitToZero(long long int n) {
	        // code here
	        return (n & 0xAAAAAAAA);
	    }
	};
	
	// { Driver Code Starts.
	int main() {
	int t;
	cin>> t;
	    while (t--) {
	        long longint n;
	cin>> n;
	        Solution ob;
	cout<<ob.convertEvenBitToZero(n) <<endl;
	    }
	    return 0;
	}
	  // }
